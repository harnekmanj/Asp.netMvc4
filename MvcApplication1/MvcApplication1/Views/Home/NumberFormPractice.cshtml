@model MvcApplication1.Models.ExpandedFormModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StandardFormPractice</title>
</head>
<body>
    <div>
        <h1>Number Form</h1>
        <table>
            @for (int i = 0; i < @Model.NumberInEnglishForm.Count; i++)
            {
            <tr>
                <td>@Model.NumberInEnglishForm[i]</td>
            </tr>
            }
        </table>
    </div>
</body>
</html>
