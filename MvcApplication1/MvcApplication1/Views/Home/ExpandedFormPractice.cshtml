@model MvcApplication1.Models.ExpandedFormModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ExpandedFormPractice</title>
</head>
<body>
    <div>
        <h1>Expanded Form</h1>
        <table>
            @for(int i=0;i< @Model.Digits.Count; i++)
            {
            <tr>
                <td>@Model.Digits[i]</td>
            </tr>
            }
        </table>
    </div>
</body>
</html>
